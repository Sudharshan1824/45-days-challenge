class Solution {
    public int missingNumber(int[] nums) {
        int sum=0;
        for(int i=0;i<nums.length;i++)
        {
            sum=sum+nums[i];
        }
        int x=0;
        int n=nums.length;
        int z=0;
        for(int i=0;i<n;i++)
        {
            x=(n)*(n+1)/2;
            z=x-sum;
        }
            return z;
    }
}
